{
  "name": "vscode-azure-storage-utility",
  "displayName": "Azure Storage Utility",
  "description": "Generates Shared Access Signature for Azure Storage URI",
  "version": "0.0.2",
  "publisher": "Compulim",
  "engines": {
    "vscode": "^0.10.1"
  },
  "icon": "icon.png",
  "galleryBanner.color": "#0D5CAB",
  "repository": {
    "type": "git",
    "url": "https://github.com/compulim/vscode-azure-storage-utility.git"
  },
  "bugs": "https://github.com/compulim/vscode-azure-storage-utility/issues/",
  "homepage": "https://github.com/compulim/vscode-azure-storage-utility/blob/master/README.md",
  "keywords": [
    "azure",
    "blob",
    "file",
    "sas",
    "storage",
    "token"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:azureStorage.buildSASURI"
